{
  "name": "turborepo-nuxt-boilerplate",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --no-daemon",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@mannesmaikel/eslint-config-custom": "workspace:*",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=v18.13.0"
  },
  "packageManager": "pnpm@7.26.3"
}
